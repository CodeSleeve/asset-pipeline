{
    "name": "codesleeve/asset-pipeline",
    "description": "A very simple and easy to use asset pipeline for laravel 4.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kelt Dockins",
            "email": "kelt@dockins.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "kriswallsmith/assetic": "v1.1.2",
        "leafo/lessphp": "v0.4.0",
        "coffeescript/coffeescript": "1.3.1",
        "nitra/php-min": "dev-master",
        "leafo/scssphp": "0.0.7"
    },
    "require-dev": {
        "mikey179/vfsStream": "1.3.*@dev",
        "mockery/mockery": "dev-master",
        "laravel/framework": "4.0.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Codesleeve\\AssetPipeline": "src/"
        }
    },
    "minimum-stability": "dev"
}
