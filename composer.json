{
    "name": "codesleeve/asset-pipeline",
    "description": "A very simple and easy to use asset pipeline for laravel 4.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kelt Dockins",
            "email": "kelt@dockins.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "codesleeve/sprockets": "v2.0.5",
        "leafo/lessphp": "v0.4.0",
        "coffeescript/coffeescript": "1.3.1",
        "nitra/php-min": "@dev",
        "leafo/scssphp": "0.0.9",
        "richthegeek/phpsass": "2014-03-20",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "laravel/framework": "4.*",
        "phpspec/phpspec": "2.0.*@dev"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-0": {
            "Codesleeve\\AssetPipeline": "src/"
        }
    },
    "minimum-stability": "dev"
}
